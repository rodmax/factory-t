{
  "name": "factory-t",
  "version": "0.2.1",
  "description": "TypeScript library for building data objects. With strong typings in mind",
  "keywords": [
    "TypeScript",
    "factory",
    "unit-test"
  ],
  "homepage": "https://github.com/rodmax/factory-t",
  "bugs": {
    "url": "https://github.com/rodmax/factory-t/issues"
  },
  "main": "build/factory-t.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "rm -rf ./build && tsc --build src",
    "examples:build": "jdi ./src/factory-t.examples.test.ts && markdown-toc -i ./src/factory-t.examples.test.ts.md",
    "examples:build:watch": "sane 'npm run examples:build' ./src --glob='*examples.test.ts'",
    "pre-push": "run-p -ln test",
    "pre-commit": "pretty-quick --staged",
    "commitlint": "commitlint",
    "changelog:build": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "version": "./tools/bump-version.sh",
    "push-and-publish": "git push && npm publish"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run pre-push",
      "pre-commit": "npm run pre-commit",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "author": "Maxim Rodionov <rodionov.m.m@gmail.com>",
  "repository": "github:rodmax/factory-t",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@types/jest": "24.0.23",
    "conventional-changelog-cli": "2.0.30",
    "cz-conventional-changelog": "3.0.2",
    "husky": "3.1.0",
    "jdi": "1.1.3",
    "jest": "24.9.0",
    "markdown-toc": "1.2.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "pretty-quick": "2.0.1",
    "sane": "4.1.0",
    "ts-jest": "24.2.0",
    "typescript": "3.8.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {}
}
