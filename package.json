{
  "name": "factory-t",
  "version": "1.1.0",
  "description": "TypeScript library for building data objects. With strong typings in mind",
  "keywords": [
    "TypeScript",
    "factory",
    "unit-test"
  ],
  "homepage": "https://github.com/rodmax/factory-t",
  "bugs": {
    "url": "https://github.com/rodmax/factory-t/issues"
  },
  "main": "build/index.js",
  "files": [
    "build/",
    "src/",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.20.4"
  },
  "scripts": {
    "build": "rm -rf ./build && tsc --build src",
    "test": "run-s test:format test:types test:lint test:md test:unit test:integration",
    "test:format": "prettier --check .",
    "test:types": "tsc --noEmit --project ./src/tsconfig.json",
    "test:unit": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:integration": "./tools/integration-test.sh",
    "test:lint": "eslint -f visualstudio ./src",
    "test:md": "run-s md:lint md:embedme:verify",
    "commitlint": "commitlint --edit",
    "changelog:build": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "version": "./tools/version.sh",
    "preversion": "./tools/pre-version.sh",
    "env:md-files": "env MD_FILES=\"\"",
    "md:lint": "markdownlint-cli2 \"./*.md\" \"./docs/*.md\" \"!CHANGELOG.md\"",
    "md:embedme": "embedme README.md docs/*.md",
    "md:embedme:verify": "npm run md:embedme -- --verify",
    "md:toc": "markdown-toc -i docs/tutorial.md",
    "md:build-all": "run-s -ln md:toc md:embedme",
    "ci:all": "npm run test",
    "pre-push": "run-p -ln test:unit md:embedme:verify",
    "pre-commit": "run-s format:staged test:lint md:lint",
    "format:staged": "pretty-quick --staged",
    "format": "pretty-quick",
    "prepare": "husky",
    "prepublishOnly": "./tools/pre-publish.sh"
  },
  "author": "Maxim Rodionov <rodionov.m.m@gmail.com>",
  "repository": "github:rodmax/factory-t",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "19.5.0",
    "@commitlint/config-conventional": "19.5.0",
    "@eslint/js": "9.14.0",
    "@types/eslint__js": "8.42.3",
    "@types/jest": "29.5.14",
    "conventional-changelog-cli": "5.0.0",
    "cz-conventional-changelog": "3.3.0",
    "embedme": "1.22.1",
    "eslint": "9.13.0",
    "eslint-formatter-visualstudio": "8.40.0",
    "eslint-plugin-jest": "28.8.3",
    "husky": "9.1.6",
    "jest": "29.7.0",
    "markdown-toc": "1.2.0",
    "markdownlint-cli2": "0.18.1",
    "npm": "10.9.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.3.3",
    "pretty-quick": "4.0.0",
    "ts-jest": "29.2.5",
    "typescript": "4.9.5",
    "typescript-eslint": "8.54.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
